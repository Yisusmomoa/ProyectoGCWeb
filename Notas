CREATE TABLE GoogleUser(
	IdGoogleUser varchar(255),
    IdGoogleUsers_usuariosFK int,
    CONSTRAINT PKGoogleUser
	primary key(IdGoogleUser),
CONSTRAINT FKIdUsuario_google
	foreign key (IdGoogleUsers_usuariosFK) references Usuario(IdUsuario)
);



